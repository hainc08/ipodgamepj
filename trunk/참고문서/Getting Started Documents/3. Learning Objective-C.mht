<P class=3Dcodesample><B>Listing 1&nbsp;&nbsp;</B>A class implementation =
</P><PRE>@implementation MyClass<SPAN></SPAN>
 <SPAN></SPAN>
- (id)initWithString:(NSString *) aName<SPAN></SPAN>
{<SPAN></SPAN>
    if (self =3D [super init]) {<SPAN></SPAN>
        count count =3D 0;<SPAN></SPAN>
        data =3D nil;<SPAN></SPAN>
        name =3D [aName copy];<SPAN></SPAN>
        return self;<SPAN></SPAN>
    }<SPAN></SPAN>
}<SPAN></SPAN>
 <SPAN></SPAN>
+ (MyClass *)createMyClassWithString: (NSString *) aName<SPAN></SPAN>
{<SPAN></SPAN>
    return [[[self alloc] initWithString:aName] =
autorelease];<SPAN></SPAN>
}<SPAN></SPAN>
@end<SPAN></SPAN>
</PRE>
